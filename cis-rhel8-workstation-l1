- id: CIS-1.1.1
  title: Disable unused filesystems
  check: "lsmod | grep cramfs"
  expected_output: ""
  remediation: "echo 'install cramfs /bin/true' >> /etc/modprobe.d/CIS.conf"
  level: 1

